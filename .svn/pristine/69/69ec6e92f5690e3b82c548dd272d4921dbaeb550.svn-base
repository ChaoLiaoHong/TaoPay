<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BoderCircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>
    <declare-styleable name="mBtn">
        <attr name="tag" format="string" />
        <attr name="num" format="float" />
    </declare-styleable>
    //流式布局
    <declare-styleable name="TagCloudLayout">
        <attr name="lineSpacing" format="dimension" />
        <attr name="tagSpacing" format="dimension" />
        <!-- 是否是固定布局 -->
        <attr name="isFixed" format="boolean" />
        <attr name="columnSize" format="integer" />
    </declare-styleable>
    //流式布局结束

    <!-- PagerSlidingTab -->
    <declare-styleable name="PagerSlidingTab">
        <attr name="indicatorColor" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="pst_dividerPadding" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tabBackground" format="reference" />
        <attr name="shouldExpand" format="boolean" />
        <attr name="pst_textAllCaps" format="boolean" />
    </declare-styleable>
    //侧滑
    <declare-styleable name="EasySwipeMenuLayout">
        <attr name="leftMenuView" format="reference" />
        <attr name="rightMenuView" format="reference" />
        <attr name="contentView" format="reference" />
        <attr name="canRightSwipe" format="boolean" />
        <attr name="canLeftSwipe" format="boolean" />
        <attr name="fraction" format="float" />
    </declare-styleable>
    //仿支付宝
    <declare-styleable name="PswInputView">
        <attr name="count" format="integer" />
        <attr name="border_color_s" format="color" />
        <attr name="dot_color" format="color" />
    </declare-styleable>
    //自定义键盘
    <declare-styleable name="NumKeyView">
        <attr name="gbColor" format="color|reference" />
        <attr name="deleteDrawable" format="reference" />
    </declare-styleable>
</resources>