<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.hmkj.taozhifu.activity.NoticeDetailsActivity">

    <include layout="@layout/common_toolbar" />

    <include layout="@layout/include_view_dp1" />


    <TextView
        android:id="@+id/tv_notice_name"
        style="@style/CommonWidthMatchParentHeightWrapContent"
        android:padding="@dimen/dp15"
        android:text="我来看看看看 "
        android:textColor="@color/black"
        android:textSize="@dimen/sp20" />

    <TextView
        android:id="@+id/tv_notice_date"
        style="@style/CommonWidthMatchParentHeightWrapContent"
        android:padding="@dimen/dp15"
        android:text="我来看看看看 "
        android:textColor="@color/j_666666"
        android:textSize="@dimen/sp12" />

    <WebView
        android:id="@+id/tv_notice_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp15"
        android:textColor="@color/j_333333"
        android:textSize="@dimen/sp14" />


</LinearLayout>
